{
    "name": "testServer",
    "version": "0.0.1",
    "private": false,
    "bin": {
        "$": "./dist/bin/app.js"
    },
    "main": "./dist/app",
    "scripts": {
        "dev": "nodemon --watch app.ts --watch app/**/* --ext .ts --exec npm run-script start",
        "start": "npm run build && cd dist &&  node app",
        "build": "node_modules/.bin/tsc",
        "test": "mocha -r ts-node/register ./app/src/redistest/*.ts --exit --timeout 2000"
    },
    "dependencies": {
        "@types/bluebird": "^3.5.19",
        "@types/mocha": "^7.0.2",
        "@types/mongoose": "^5.7.32",
        "@types/node": "8.10.54",
        "@types/redis": "^2.8.14",
        "@types/should": "^13.0.0",
        "@types/underscore": "^1.10.0",
        "bluebird": "^3.5.1",
        "mocha": "^7.2.0",
        "mongoose": "^5.9.19",
        "pinus": "1.4.1",
        "pm2": "^4.4.0",
        "pomelo-cli": "^0.1.4",
        "pomelo-masterha-plugin": "0.0.4",
        "reflect-metadata": "^0.1.10",
        "should": "^13.2.3",
        "source-map-support": "^0.5.0",
        "ts-node": "^8.6.2",
        "typegoose": "^5.9.1"
    },
    "devDependencies": {
        "nodemon": "^2.0.4",
        "redis": "^2.8.0",
        "tslint": "5.20.1",
        "typescript": "3.7.3"
    }
}
